{
  "name": "eleniarvanitis.com",
  "repository": "github.com/DatGreekChick/personal",
  "version": "0.0.1",
  "description": "My personal website featuring my portfolio and my writing pieces.",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd functions && npm install",
    "prepare": "firebase use --add && echo 'module.exports = firebase =>' > fire/setup.js && npx firebase setup:web >> fire/setup.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dev",
    "build": "webpack -p",
    "watch-lib": "npm run build-lib -- -w",
    "build-lib": "webpack --config lib/webpack.config.js",
    "build-lib-prod": "npm run build-lib -- -p",
    "build-prod": "npm run build-lib-prod && npm run build",
    "deploy": "npm run build-prod && npx firebase deploy"
  },
  "keywords": [
    "personal-website",
    "react",
    "redux",
    "react-redux",
    "firebase",
    "portfolio",
    "work",
    "projects",
    "contact",
    "personal-branding",
    "branding"
  ],
  "author": "Eleni Arvanitis",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.3.0",
    "firebase-tools": "^3.16.0",
    "react-hot-loader": "^3.1.2",
    "strip-ansi": "^4.0.0",
    "through2": "^2.0.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.6",
    "firebase": "^4.8.0",
    "firebase-admin": "^5.5.1",
    "minimist": "^1.2.0",
    "npm": "^5.6.0",
    "react": "^16.2.0",
    "react-async-script": "^0.9.1",
    "react-dom": "^16.2.0",
    "react-google-recaptcha": "^0.9.9",
    "react-router-dom": "^4.2.2",
    "style-loader": "^0.19.1",
    "t2-cli": "^0.1.17",
    "url-loader": "^0.6.2"
  }
